    <header class="flex justify-between items-center mb-8">
      <%= link_to "Ancient", root_path, class: "text-2xl font-bold" %>
      <nav>
        <ul class="flex justify-between items-center w-full">
            <li class="px-4 py-2"><%= link_to "Users", users_path %></li>
          <% if user_signed_in? %>
            <li class="px-4 py-2"><%= link_to current_user.email, edit_user_path(current_user) %></li>
            <li class="px-4 py-2"><%= link_to 'Log Out', logout_path, data: { turbo_method: :delete } %></li>
          <% else %>
            <li class="px-4 py-2"><%= link_to 'Log In', new_session_path %></li>
          <% end %>
        </ul>
      </nav>
    </header>